{
    "version": 2,
    "builds": [
    {
        "src": "dynamicBadges",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "15mb"
        }
    }
    ],
    "routes": [
    {
        "src": "/badge/stars/(.*)/(.*)/(.*)/(.*)",
        "dest": "dynamicBadges/main.py"
    }
    ]
}